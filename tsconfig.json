{
  "compilerOptions": {
    "module": "ESNext",
    "target": "es2021",
    "jsx": "react-jsx",
    "allowImportingTsExtensions": true,
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
  },
  "include": ["src"]
}
